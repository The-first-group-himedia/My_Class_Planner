<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mcp.myclassplanner.model.dao.CourseMapper">
    <insert id="addCourse" parameterType="mcp.myclassplanner.model.entity.CourseEntity"
       useGeneratedKeys="true" keyProperty="courseNo">
            insert into tbl_course
            (
            member_code,
            course_name,
            section_start,
            section_end,
            section_days
            )
            VALUES
            (
            1,
            #{courseName},
            #{startTime},
            #{endTime},
            #{days}
            )

    </insert>
</mapper>